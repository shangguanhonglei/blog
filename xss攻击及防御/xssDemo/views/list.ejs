<!DOCTYPE html>
<html>

<head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
    <h1><%= title %></h1>
    
    <% posts.forEach(function (post, index) { %>
        <p><h2><a href="#"><%= post.title %></a></h2></p>
        <p class="info">
          日期：<%= post.time.minute %>
        </p>
        <p><%- post.post %></p>
      <% }) %>
</body>

</html>